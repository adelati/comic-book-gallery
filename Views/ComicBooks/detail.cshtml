@{
    Layout = null;

}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
    <div>
    <h1>@ViewBag.SeriesTitle</h1>
    <h2>Issue #@ViewBag.IssueNumber </h2>

    <h5>Description</h5>
    <div>@Html.Raw(ViewBag.Description)</div>
    @if (ViewBag.ArtistsList.Length != 0) {
        <h5>Artists:</h5>
	    <ul>
	    	@foreach(var artist in ViewBag.ArtistsList){
	    		<li>@artist</li>
	    	}
	    </ul>
    	
    }
    		


    </div>
</body>
</html>

